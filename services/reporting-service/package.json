{
  "name": "reporting-service",
  "version": "1.0.0",
  "description": "Comprehensive reporting and analytics service for CloudPOS system",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "dev:watch": "nodemon --exec ts-node src/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js"
  },
  "keywords": [
    "reporting",
    "analytics",
    "dashboard",
    "business-intelligence",
    "pos",
    "retail"
  ],
  "author": "CloudPOS Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "decimal.js": "^10.4.3",
    "pg": "^8.11.3",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "cron": "^3.1.6",
    "nodemailer": "^6.9.7",
    "pdf-lib": "^1.17.1",
    "exceljs": "^4.4.0",
    "csv-writer": "^1.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.5",
    "@types/joi": "^17.2.3",
    "@types/nodemailer": "^6.4.14",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudpos/reporting-service.git"
  },
  "bugs": {
    "url": "https://github.com/cloudpos/reporting-service/issues"
  },
  "homepage": "https://github.com/cloudpos/reporting-service#readme"
}