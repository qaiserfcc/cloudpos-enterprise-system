# Frontend Admin Dashboard Environment Template
# Copy this file to .env for local development

# API Configuration
VITE_API_URL=http://localhost:3000
VITE_AUTH_SERVICE_URL=http://localhost:3001
VITE_TRANSACTION_SERVICE_URL=http://localhost:3003
VITE_INVENTORY_SERVICE_URL=http://localhost:3004
VITE_PAYMENT_SERVICE_URL=http://localhost:3005
VITE_CUSTOMER_SERVICE_URL=http://localhost:3006
VITE_NOTIFICATION_SERVICE_URL=http://localhost:3007
VITE_REPORTING_SERVICE_URL=http://localhost:3008

# Development Configuration
VITE_NODE_ENV=development
VITE_API_TIMEOUT=10000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# External Services
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key

# Security
VITE_ENCRYPTION_KEY=your_encryption_key_here
VITE_API_KEY=your_api_key_here